{
  "version": 1,
  "files": [
    "NLLB-v1.bs-en.bs.gz",
    "NLLB-v1.bs-en.en.gz"
  ],
  "filters": [
    {
      "filter": "fix_quotes",
      "parameters": {},
      "language": null
    },
    {
      "filter": "fix_wiki",
      "parameters": {
        "ALWAYS": false,
        "FOOTNOTES": true,
        "URLS": true,
        "WIKILINKS": true,
        "CODE": true,
        "HEADINGS": true,
        "REMOVEEMPTYLINES": true
